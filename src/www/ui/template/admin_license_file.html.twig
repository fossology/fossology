{% extends "include/base.html.twig" %}

{% block content %}

  {{ Inputfm }}

  <p>{{ dataMessage }}</p>
  {% if data %}
  <table id='adminLicenseTable' rules='rows' width='100%' style="width:100%" cellpadding='3'>
    <tfoot class="footAsHead">
      <tr style="background:lightsteelblue">
        <td colspan="2" style="width:12%"><strong>Search:</strong></td>
        <th style="width:12%">SPDX ID</th>
        <th style="width:12%">Shortname</th>
        <th style="width:20%">Fullname</th>
        <th style="width:20%">Text</th>
        <th style="width:10%">URL</th>
        <th style="width:14%">Obligation topics</th>
      </tr>
    </tfoot>
  </table>
  {% endif %}
{% endblock %}

{% block foot %}
  {{ parent() }}
  <script src="scripts/jquery.dataTables.min.js" type="text/javascript"></script>
  <script src="scripts/jquery.dataTables.select.js" type="text/javascript"></script>
  {% if data %}
  <script type="text/javascript"> {% include 'admin_license_file.js.twig' %}</script>
  {% endif %}
{% endblock %}
