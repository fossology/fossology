{
  "@type": "Package",
  "@id": "{{ uri }}#SPDXRef-upload{{ packageId }}",
  "name": "{{ packageName }}",
  "verifiedUsing": [
    {
      "@type": "Hash",
      "algorithm": "sha1",
      "hashValue": "{{ sha1 | lower }}"
    },
    {
      "@type": "Hash",
      "algorithm": "sha256",
      "hashValue": "{{ sha256 | lower }}"
    },
    {
      "@type": "Hash",
      "algorithm": "md5",
      "hashValue": "{{ md5 | lower }}"
    }
  ]
},

{{ fileNodes }}

